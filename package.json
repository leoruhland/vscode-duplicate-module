{
    "name": "duplicate-module",
    "displayName": "Duplicate Module",
    "description": "Duplicate a folder, replace all strings and rename all files in this folder.",
    "version": "0.0.1",
    "publisher": "leoruhland",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.duplicateModule"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.duplicateModule",
                "title": "Duplicate Module"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "explorerResourceIsFolder",
                    "command": "extension.duplicateModule",
                    "group": "DuplicateModule@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.2",
        "@types/lodash.escaperegexp": "^4.1.3",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "change-case": "^3.0.2",
        "fs-extra": "^6.0.1",
        "lodash.escaperegexp": "^4.1.2"
    }
}
